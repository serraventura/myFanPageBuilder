{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch node express",
            "type": "node",
            "request": "launch",
            // "program": "${workspaceRoot}/server/node_modules/nodemon/bin/nodemon.js",
            // "args": [
            //     "${workspaceRoot}/server/bin/www"
            // ],
            "program": "${workspaceRoot}/server/bin/www",
