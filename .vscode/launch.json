{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch nodemon",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/server/node_modules/nodemon/bin/nodemon.js",
            "cwd": "${workspaceRoot}",
            "args": [
                "${workspaceRoot}/server/bin/www"
            ],
            "runtimeArgs": [
                "--nolazy"
            ],
            "env": {
                "NODE_ENV": "development"
            },
            "stopOnEntry": false,
            "sourceMaps": false,
            "outDir": "${workspaceRoot}/server/outputlog"
        }
    ]
}